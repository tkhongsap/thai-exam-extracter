# Thai Exam Extractor - Environment Variables
# Copy this file to .env and update with your values

# API Configuration
# Base URL for the exam API
EXAM_API_BASE_URL=https://www.trueplookpanya.com/webservice/api/examination/formdoexamination

# API timeout in seconds
EXAM_API_TIMEOUT=30

# Maximum number of retries for failed API requests
EXAM_API_MAX_RETRIES=3

# Download Configuration
# Number of concurrent downloads
EXAM_CONCURRENT_LIMIT=5

# Delay between requests in seconds (rate limiting)
EXAM_RATE_LIMIT_DELAY=0.5

# Output Configuration
# Directory to save extracted exam data
EXAM_OUTPUT_DIR=data/output

# Extraction Range
# Start exam ID
EXAM_START_ID=14000

# End exam ID
EXAM_END_ID=20000
